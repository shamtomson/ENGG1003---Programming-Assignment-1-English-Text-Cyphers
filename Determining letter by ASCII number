#include <stdio.h>

int main(){
    
    char c;
    printf("enter a letter:\n ");
    scanf("%c", &c);
    c = c - 97;
    switch (c){
    case 1:
     printf("B\n");
     break;
    case 2:
     printf("C\n");
     break;
    case 3:
     printf("D\n");
     break;
    case 4:
     printf("E\n");
     break;
    case 5:
     printf("F\n");
     break;
    case 6:
     printf("G\n");
     break;
    case 7:
     printf("H\n");
     break;
    case 8:
     printf("I\n");
     break;
    case 9:
     printf("J\n");
     break;
    case 10:
     printf("k\n");
     break;
    default:
        printf("A\n");
        break;
    }
   return 0;
    
}
